<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>Feed_Status</servlet-name>
		<servlet-class>com.feedstatus.Feed_StatusServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Feed_Status</servlet-name>
		<url-pattern>/feed_status</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Logout</servlet-name>
		<servlet-class>com.feedstatus.Logout</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Signup</servlet-name>
		<servlet-class>com.feedstatus.Signup</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Profile</servlet-name>
		<servlet-class>com.feedstatus.Profile</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Profile</servlet-name>
		<url-pattern>/profile</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>Signup</servlet-name>
		<url-pattern>/signup</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Logout</servlet-name>
		<url-pattern>/Logout</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
